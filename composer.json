{
    "name": "clemblanco/dusk-scraper",
    "description": "Laravel Dusk Scraper is Laravel Dusk for scrapping only.",
    "keywords": ["laravel", "scraper", "crawler", "webdriver"],
    "license": "MIT",
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        },
        {
            "name": "ClÃ©ment Blanco",
            "email": "mail@clem.io"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "ext-json": "*",
        "ext-zip": "*",
        "facebook/webdriver": "^1.7",
        "nesbot/carbon": "^1.20|^2.0",
        "illuminate/console": "~5.7.0|~5.8.0|^6.0|^7.0",
        "illuminate/support": "~5.7.0|~5.8.0|^6.0|^7.0",
        "symfony/console": "^4.0",
        "symfony/finder": "^4.0",
        "symfony/process": "^4.0",
        "vlucas/phpdotenv": "^2.2|^3.3"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.5|^8.0"
    },
    "autoload": {
        "psr-4": {
            "DuskScraper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DuskScraper\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.0-dev"
        },
        "laravel": {
            "providers": [
                "DuskBrowser\\ServiceProvider"
            ],
            "aliases": {
                "DuskScraper": "DuskScraper\\Facades\\DuskScraper"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
