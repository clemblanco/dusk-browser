{
    "name": "dusk-browser/dusk-browser",
    "description": "Laravel Dusk Browser is Laravel Dusk for browsing capabilities only.",
    "keywords": ["laravel", "browser", "scraper", "crawler", "webdriver"],
    "license": "MIT",
    "authors": [
        {
            "name": "ClÃ©ment Blanco",
            "email": "mail@clem.io"
        },
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "ext-json": "*",
        "ext-zip": "*",
        "facebook/webdriver": "^1.7",
        "nesbot/carbon": "^1.20|^2.0",
        "illuminate/console": "~5.7.0|~5.8.0|^6.0|^7.0",
        "illuminate/support": "~5.7.0|~5.8.0|^6.0|^7.0",
        "symfony/console": "^4.0",
        "symfony/finder": "^4.0",
        "symfony/process": "^4.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.5|^8.0"
    },
    "autoload": {
        "psr-4": {
            "DuskBrowser\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DuskBrowser\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DuskBrowser\\ServiceProvider"
            ],
            "aliases": {
                "DuskBrowser": "DuskBrowser\\Facades\\DuskBrowser"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
